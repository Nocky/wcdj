include ../../Inc.mk

LIB_HTTP     = ../../../lib/libhttp.a
OBJS_HTTP    = http_handle.o

DEP_PATH      = ../../dep
CURL_API_PATH = $(DEP_PATH)/curl/osx64_gcc421/curl-7.36.0

INCLUDE += -I$(CURL_API_PATH)/include

LIBS = $(LIB_HTTP) 
OBJS = $(OBJS_HTTP) 

all: $(LIBS)

$(LIB_HTTP): $(OBJS_HTTP)
	$(AR) $(AROPT) $@ $? 
	$(RANLIB) $@


install:
	@echo "nothing to install"
		
clean:
	rm -f $(OBJS) $(LIBS)

