COLLECT_GCC=./g++
COLLECT_LTO_WRAPPER=/Users/gerryyang/LAMP/gcc/install/gcc-4.9.0/libexec/gcc/x86_64-apple-darwin13.1.0/4.9.0/lto-wrapper
